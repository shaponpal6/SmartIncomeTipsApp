{
  "expo": {
    "name": "Smart Income Tips",
    "slug": "smart-income-tips",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#001c30"
    },
    "ios": {
      "bundleIdentifier": "tips.smartincome",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "tips.smartincome",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#001c30"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
